{
  "name": "Strava Unit Converter",
  "description": "This allows you to view both units on your strava feed.",
  "manifest_version": 3,
  "version": "1.1.1",
  "content_scripts": [
    {
      "matches": ["*://*.strava.com/*"],
      "run_at": "document_end",
      "js": ["converter.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "strava-unit-converter@raymond"
    }
  }
}
